{
    "name": "openparliamentbe/core",
    "description": "The core of the OpenParliament.be project.",
    "license": "AGPL-3.0",
    "type": "project",
    "support": {
        "source": "https://github.com/openparliamentbe/core",
        "issues": "https://github.com/openparliamentbe/core/issues"
    },
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.1.*",
        "vlucas/phpdotenv": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
